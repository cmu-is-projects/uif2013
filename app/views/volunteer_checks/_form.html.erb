<%= simple_form_for(@volunteer_check) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :volunteer_id, :collection => Volunteer.alphabetical %>
    <%= f.input :check_id, :collection => Check.alphabetical %>
	<%= f.input :date_submitted, :order => [:month, :day, :year], :start_year => Date.today.year-10, :end_year => Date.today.year %>
	<%= f.input :date_completed, :order => [:month, :day, :year], :start_year => Date.today.year-10, :end_year => Date.today.year %>
	<%= f.input :paid %>
	<%= f.input :approved %>
    <%= f.input :notes %>

      
</div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
