<%= simple_form_for(@volunteer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :barcode_number %>
    <%= f.input :household_id %>
    <%= f.input :is_male %>
    <%= f.input :date_of_birth %>
    <%= f.input :cell_phone %>
    <%= f.input :can_text %>
    <%= f.input :email %>
    <%= f.input :status %>
    <%= f.input :avatar_file_name %>
    <%= f.input :avatar_file_size %>
    <%= f.input :avatar_file_content_type %>
    <%= f.input :avatar_updated_at %>
    <%= f.input :app_submit_date %>
    <%= f.input :app_approved %>
    <%= f.input :role %>
    <%= f.input :name_displayed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
